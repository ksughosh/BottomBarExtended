<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomBarView">
        <!-- The menu resource to inflate and populate items from. -->
        <attr name="menu" format="reference" />
        <attr name="itemIconTint" />
        <attr name="itemTextColor" />
        <attr name="itemBackground" />
        <attr name="mode" format="enum">
            <enum name="shift" value="0" />
            <enum name="scale" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="elevation" />
        <attr name="disableText" format="boolean" />
        <attr name="badgeLayout" format="reference" />
        <attr name="badgeDrawable" format="reference" />
        <attr name="badgeItemId" format="integer" />
    </declare-styleable>
</resources>